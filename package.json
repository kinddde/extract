    "onchange": "^6.0.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  }
