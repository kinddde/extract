{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "8.0.0"
        }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "regenerator": true,
        "corejs": 3
      }
    ]
  ]
}
